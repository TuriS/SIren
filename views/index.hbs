<div class="container-fluid">
    {{!--
    <h1>{{title}}</h1>
    <p>Welcome to {{title}}</p> --}}
    <div class="container">
        <div class="row">
            {{!--
            <p class="card-text"></p> --}}
            <div class="col-1"><button class="btn btn-primary" onclick="start(0)"><span class="glyphicon glyphicon-play"></span></button></div>
